  end)

  local aItem = {[1] = 'Beer Bottle', [2] = 'Golden Chest', [5] = 'White Chest', [6] = 'Pressed Flower', [7] = 'Illustration',
                [13] = 'Royal Correspondence', [14] = 'Secret of London', [18] = 'Helix glitch', [9] = 'Letter f/t Front'}

  local fPrevCoordSum = 0
