      local yCoord = f.CoordY
      yCoord.Caption = ''
      local zCoord = f.CoordZ
      zCoord.Caption = ''
      f.PlayerX.Caption = ''
      f.PlayerY.Caption = ''
      f.PlayerZ.Caption = ''

      lbTitle.Caption = string.format('UIMinimapManager: {%X+%X}',getAddress("[pUIMinimapMgr]"),0x60)
      mr = al.getMemoryRecordByID(26077)
      local sInfo = mr.DropDownList[nChoice+1]
      sInfo = sInfo:match(":(.*)")
